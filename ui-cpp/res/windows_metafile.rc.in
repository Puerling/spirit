#include "winver.h"

IDI_ICON1               ICON    DISCARDABLE     "@ICON_FILE@"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @SPIRIT_META_VERSION_MAJOR@-@SPIRIT_META_VERSION_MINOR@-@SPIRIT_META_VERSION_PATCH@
 PRODUCTVERSION @SPIRIT_META_VERSION_MAJOR@-@SPIRIT_META_VERSION_MINOR@-@SPIRIT_META_VERSION_PATCH@
 FILEFLAGS 0x0L
 FILEFLAGSMASK 0x3fL
 FILEOS 0x00040004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "CompanyName", "@META_AUTHOR_ORGANIZATION@"
            VALUE "FileDescription", "@META_PROJECT_NAME@"
            VALUE "FileVersion", "@META_VERSION@"
            VALUE "InternalName", "@META_PROJECT_NAME@"
            VALUE "OriginalFilename", "@META_PROJECT_NAME@.exe"
            VALUE "ProductName", "@META_PROJECT_NAME@"
            VALUE "ProductVersion", "@META_VERSION@"
        END
    END
END

